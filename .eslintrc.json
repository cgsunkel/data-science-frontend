{
  "extends": "eslint:recommended",

  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },

  "parserOptions": {
    "ecmaVersion": 8
  },

  "rules": {
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "never",
        "exports": "never",
        "functions": "ignore"
      }
    ],
    "object-curly-spacing": ["error", "always"],
    "dot-notation": "error"
  }
}
