{
  "env": {
    "jest": true
  },
  "extends": "standard",

  "rules": {
    "semi": [1, "always"],
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "never",
        "exports": "never",
        "functions": "ignore"
      }
    ],
    "dot-notation": "error"
  }
}
